{
  "name": "cli-password-checker",
  "version": "1.1.3",
  "description": "A Node.js CLI tool to validate and generate passwords based on configurable criteria.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:linux": "pkg . --target node18-linux-x64 --output cliPass2",
    "build:win": "pkg . --target node18-win-x64 --output cliPass.exe",
    "test": "jest"
  },
  "bin": {
    "cliPass": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LelonDelonMelon/cliPassCheck.git"
  },
  "keywords": [
    "password",
    "generator",
    "validator",
    "cli",
    "nodejs",
    "node"
  ],
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "pkg": {
    "assets": [
      "config/*.cpc"
    ]
  },
  "author": "LelonDelonMelon",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
